{
  "name": "React-RCTFBReactNativeSpec",
  "version": "0.81.4",
  "summary": "FBReactNativeSpec for React Native.",
  "homepage": "https://reactnative.dev/",
  "license": "MIT",
  "authors": "Meta Platforms, Inc. and its affiliates",
  "platforms": {
    "ios": "15.1"
  },
  "source": {
    "git": "https://github.com/facebook/react-native.git",
    "tag": "v0.81.4"
  },
  "source_files": "FBReactNativeSpec/**/*.{h}",
  "compiler_flags": "-DRCT_NEW_ARCH_ENABLED=1",
  "exclude_files": [
    "FBReactNativeSpec/react/renderer/components",
    " -DRCT_NEW_ARCH_ENABLED=1"
  ],
  "header_dir": "FBReactNativeSpec",
  "pod_target_xcconfig": {
    "USE_HEADERMAP": "NO",
    "CLANG_CXX_LANGUAGE_STANDARD": "c++20",
    "HEADER_SEARCH_PATHS": "\"$(PODS_TARGET_SRCROOT)/FBReactNativeSpec\" \"$(PODS_ROOT)/Headers/Private/Yoga\" \"${PODS_ROOT}/ReactCommon\" \"${PODS_ROOT}/ReactCommon/react/nativemodule/core\" \"${PODS_ROOT}/ReactCommon\" \"${PODS_ROOT}/ReactCommon/react/nativemodule/bridging\""
  },
  "dependencies": {
    "React-jsi": [],
    "RCTRequired": [],
    "RCTTypeSafety": [],
    "React-Core": [],
    "React-NativeModulesApple": [],
    "ReactCommon": [],
    "hermes-engine": [],
    "ReactNativeDependencies": [],
    "React-Core-prebuilt": []
  },
  "script_phases": [
    {
      "name": "[RN]Check FBReactNativeSpec",
      "execution_position": "before_compile",
      "always_out_of_date": "1",
      "script": "echo \"Checking whether Codegen has run...\"\nfbReactNativeSpecPath=\"$REACT_NATIVE_PATH/React/FBReactNativeSpec\"\n\nif [[ ! -d \"$fbReactNativeSpecPath\" ]]; then\n  echo 'error: Codegen did not run properly in your project. Please reinstall cocoapods with `bundle exec pod install`.'\n  exit 1\nfi\n"
    }
  ],
  "subspecs": [
    {
      "name": "components",
      "source_files": "FBReactNativeSpec/react/renderer/components/FBReactNativeSpec/**/*.{h}",
      "header_dir": "react/renderer/components/FBReactNativeSpec",
      "pod_target_xcconfig": {
        "HEADER_SEARCH_PATHS": "\"${PODS_ROOT}/React-featureflags\" \"${PODS_ROOT}/React-debug\" \"${PODS_ROOT}/React-rendererdebug\" \"${PODS_ROOT}/React-utils\" \"${PODS_ROOT}/React-graphics\" \"${PODS_ROOT}/React-graphics/react/renderer/graphics/platform/ios\" \"${PODS_ROOT}/React-Fabric\" \"${PODS_ROOT}/React-Fabric/react/renderer/components/view/platform/cxx\""
      },
      "dependencies": {
        "React-featureflags": [],
        "React-debug": [],
        "React-rendererdebug": [],
        "React-utils": [],
        "React-graphics": [],
        "React-Fabric": [],
        "Yoga": []
      }
    }
  ]
}
